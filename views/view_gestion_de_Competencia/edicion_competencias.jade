- var menu="", crear="",edicion="active";


include ./menu.jade
input(class="form-control" id="myInput" type="text" placeholder="Search..")

 
table(class="table" )
    thead
        tr
            th(scope="col") # 
            th(scope="col") Competencia
            th(scope="col") Accion
    tbody(id="myTable")
        each competencia in nombre_competencia_programa          
            tr
                td=competencia.id_gestion_de_competencia
                td=competencia.nombre_competencia
                td                          
                    a( class="btn btn-outline-danger" href="/gestion_de_competencia?codigo=#{competencia.id_gestion_de_competencia}&accion=Eliminar&#{direccionProgramaFormacion}") Eliminar
                    a( class="btn btn-outline-warning" href="/gestion_de_competencia?codigo=#{competencia.id_gestion_de_competencia}&accion=Actualizar&#{direccionProgramaFormacion}") Actualizar
                    a( class="btn btn-outline-success" href="/gestion_de_resultado_aprendizaje/all/#{competencia.id_gestion_de_competencia}?#{direccionProgramaFormacion}" title="Agregar resultado de aprendizaje") Agregar RA

