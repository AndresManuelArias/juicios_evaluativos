- var menu="", crear="",edicion="active";

include ./menu.jade
input(class="form-control" id="myInput" type="text" placeholder="Search..")

table(class="table" )
    thead
        tr
            th(scope="col") # 
            th(scope="col") resultado aprendizaje
            th(scope="col") Accion
    tbody(id="myTable")
        each resultado in resultadosAprendizaje          
            tr
                td=resultado.id_resultado_de_aprendizaje
                td=resultado.nombre_resultado_de_aprendizaje
                td                          
                    a( class="btn btn-outline-danger" href="/gestion_de_resultado_aprendizaje/#{id_gestion_de_competencia}?id_resultado_de_aprendizaje=#{resultado.id_resultado_de_aprendizaje}&accion=Eliminar&#{direccionProgramaFormacion}") Eliminar
                    a( class="btn btn-outline-warning" href="/gestion_de_resultado_aprendizaje/#{id_gestion_de_competencia}?id_resultado_de_aprendizaje=#{resultado.id_resultado_de_aprendizaje}&accion=Actualizar&#{direccionProgramaFormacion}") Actualizar


